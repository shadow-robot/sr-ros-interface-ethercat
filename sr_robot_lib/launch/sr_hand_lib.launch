<launch>
  <!-- For the moment the sensor_to_joint mapping will be the same for all the existing models of hand -->
  <rosparam command="load" file="$(find sr_edc_launch)/mappings/sensor_to_joint.yaml" />
  <!-- The joint_to_motor_mapping is different depending on the hand being a right or a left hand -->
  <rosparam command="load" file="$(find sr_edc_launch)/mappings/joint_to_motor_mapping.yaml" />
  <!-- The joint_to_muscle_mapping is different depending on the hand being a right or a left hand -->
  <rosparam command="load" file="$(find sr_edc_launch)/mappings/joint_to_muscle_mapping.yaml" />
  <!-- The calibration of the position sensors is different for every hand -->
  <rosparam command="load" file="$(find sr_ethercat_hand_config)/calibrations/calibration.yaml" />
  <!-- The calibration of the pressure sensors is different for every hand -->
  <rosparam command="load" file="$(find sr_ethercat_hand_config)/calibrations/pressure_calibration.yaml" />
</launch>
